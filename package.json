{
	"name": "line-linker",
	"publisher": "MichaelCarolin",
	"displayName": "line-linker",
	"description": "Quickly get an app link to a file and line number in vscode",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.linkLine"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.linkLine",
				"title": "Copy link to line"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "extension.linkLine",
					"title": "Link line",
					"group": "9_cutcopypaste"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.43.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"jsutils": "https://github.com/lancetipton/jsutils.git#v4.0.0",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"clipboardy": "^2.3.0"
	}
}
